{
  "name": "D_ProjectName_D",
  "version": "0.0.1",
  "description": "Change me pls",
  "main": "index.ts",
  "engines": {
    "node": ">=10.22.0"
  },
  "scripts": {
    "test": "TS_NODE_FILES=true mocha -r ts-node/register -r tsconfig-paths/register src/**/*.spec.ts",
    "test:coverage": "nyc --reporter=lcov --reporter=text-summary yarn test",
    "lint": "eslint ./src",
    "lint:fix": "yarn lint --fix",
    "prettier": "npx prettier -c './src/**/*.ts'",
    "prettier:fix": "npx prettier --write './src/**/*.ts'",
    "prettier:quick": "npx pretty-quick --staged",
    "start:dev": "node --inspect -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "dev": "yarn start:dev",
    "release": "npx standard-version --release-as",
    "clean": "rm -rf dist/ build/",
    "build": "yarn clean && tsc && yarn build:webpack",
    "build:webpack": "webpack --config webpack.config.js",
    "prepare": "husky install"
  },
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.8",
    "@types/express-winston": "^4.0.0",
    "@types/mocha": "^8.0.3",
    "@types/sinon": "^9.0.9",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chai": "^4.2.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "husky": "^7.0.2",
    "lint-staged": "^10.4.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "prettier-quick": "^0.0.5",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.9.4",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "agentkeepalive": "^4.1.4",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status": "^1.4.2",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.8.2"
  },
  "lint-staged": {
    "*.ts": ["yarn lint:fix"]
  }
}
